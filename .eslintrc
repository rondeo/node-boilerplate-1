{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "prettier",
    "prettier/standard"
  ],
  "plugins": [
    "prettier",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2017 
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": 2
  }
}
